{
  "build": {
    "appId": "xyz.keflix.keflix_admin_panel",
    "files": [
      "build/icon.*",
      "build/installerSidebar.*"
    ],
    "directories": {
      "buildResources": "build"
    },
    "mac": {
      "category": "public.app-category.productivity"
    },
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "createDesktopShortcut": "always"
    }
  },
  "homepage": "./",
  "name": "keflix-admin-panel",
  "version": "1.0.1",
  "productName": "Keflix Admin Panel",
  "description": "Keflix Admin Panel",
  "license": "MIT",
  "author": {
    "name": "https://github.com/adri1336",
    "email": "adri1pawn@gmail.com",
    "url": "https://github.com/adri1336"
  },
  "repository": "https://github.com/adri1336/keflix_admin",
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "electron-dl": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "get-video-duration": "^3.0.2",
    "i18next": "^19.4.4",
    "react": "^16.13.1",
    "react-contenteditable": "^3.3.4",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "youtube-dl": "^3.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --ia32 --x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "wait-on": "^5.0.0"
  }
}
